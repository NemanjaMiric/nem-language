# Fibonacci numbers generator

function fib(first, second, stop) (
    while (first < stop) (
        temporary = first
        first = second
        second = temporary + second
    )

    return 0
)

fib(1, 1, 1000)

# Higher-order function combined with function list indexing

function hof() (
    function inner_func() (
        return [[1, 2], 2]
    )
    return inner_func
)

hof()()[0][1]
